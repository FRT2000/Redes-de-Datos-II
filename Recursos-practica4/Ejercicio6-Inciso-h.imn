node n5 {
    type router
    model host
    network-config {
	hostname Server
	!
	interface eth0
	 ipv6 address 2001:1::10/64
	 ip address 192.168.1.10/24
	!
    }
    canvas c1
    iconcoords {703.0 178.0}
    labelcoords {703.0 210.0}
    interface-peer {eth0 n1}
}

link l1 {
    nodes {n3 n2}
    bandwidth 0
}

link l2 {
    nodes {n4 n2}
    bandwidth 0
}

link l3 {
    nodes {n2 n1}
    bandwidth 0
}

link l4 {
    nodes {n1 n5}
    bandwidth 0
}

