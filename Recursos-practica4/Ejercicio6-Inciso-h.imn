node n1 {
    type router
    model router
    network-config {
	hostname Router
	!
	interface eth1
	 ipv6 address 2001:1::1/64
	 ip address 192.168.1.1/24
	!
	interface eth0
	 ip address 10.0.0.1/24
	 ipv6 address 2001:0::1/64
	!
    }
    canvas c1
    iconcoords {453.0 178.0}
    labelcoords {453.0 210.0}
    interface-peer {eth0 n2}
    interface-peer {eth1 n5}
}

node n2 {
    type lanswitch
    network-config {
	hostname Switch
	!
    }
    canvas c1
    iconcoords {282.0 176.0}
    labelcoords {282.0 200.0}
    interface-peer {e0 n3}
    interface-peer {e1 n4}
    interface-peer {e2 n1}
}

