node n2 {
    type router
    model router
    network-config {
	hostname n2
	!
	interface eth0
	 ip address 10.0.0.2/24
	 ipv6 address 2001:0::2/64
	!
	interface eth1
	 mac address 02:33:22:00:00:20
	!
    }
    canvas c1
    iconcoords {505.0 118.0}
    labelcoords {505.0 150.0}
    interface-peer {eth1 n4}
    services {IPForward}
    interface-peer {eth0 n1}
}

